  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "devmoji -e",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "prettier --write",
      "jest --bail --findRelatedTests",
      "eslint --fix"
    ]
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true
    }
  }
